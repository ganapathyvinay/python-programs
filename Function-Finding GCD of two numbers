def gcd(x,y):
  '''
  The function giving Greatest common divisor of two numbers x and y
  '''
  try:
    f_num=int(x)
    s_num=int(y)
  except:
    return("Enter the integral values for x and y : ")
    quit()
  gcd=0
  i=1
  while i<=min(f_num,s_num):
    if f_num%i==0 and s_num%i==0:
      gcd=i
    i=i+1
  return(gcd)
